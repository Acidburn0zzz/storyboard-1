{
  "name": "storyboard",
  "version": "0.0.1",
  "description": "End-to-end, hierarchical, real-time, colorful logs & stories",
  "main": "index.js",
  "scripts": {
    "compile": "rm -rf dist && coffee -o dist -c src",
    "buildExtension": "webpack --config src/chromeExtension/webpackConfig.coffee --colors --progress --display-modules --display-chunks",
    "buildExtensionWatch": "webpack --config src/chromeExtension/webpackConfig.coffee --colors --progress --display-modules --display-chunks --watch",
    "buildServerLogsApp": "webpack --config src/serverLogsApp/webpackConfig.coffee --colors --progress --display-modules --display-chunks",
    "buildServerLogsAppWatch": "webpack --config src/serverLogsApp/webpackConfig.coffee --colors --progress --display-modules --display-chunks --watch",
    "buildExampleWatch": "webpack --config src/example/webpackConfig.coffee --colors --progress --display-modules --display-chunks --watch",
    "build": "npm run compile && npm run buildExtension && npm run buildServerLogsApp",
    "start": "coffee src/example/server.coffee"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/storyboard.git"
  },
  "keywords": [
    "log",
    "logging",
    "websockets",
    "console",
    "isomorphic"
  ],
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guigrpa/storyboard/issues"
  },
  "dependencies": {
    "bluebird": "3.3.1",
    "express": "4.13.4",
    "lodash": "4.5.0",
    "socket.io": "1.4.5",
    "timm": "0.4.0"
  },
  "homepage": "https://github.com/guigrpa/storyboard#readme",
  "devDependencies": {
    "body-parser": "1.15.0",
    "chai": "3.5.0",
    "chalk": "1.1.1",
    "cjsx-loader": "2.1.0",
    "coffee-loader": "0.7.2",
    "coffee-script": "1.10.0",
    "css-loader": "0.23.1",
    "envify": "3.4.0",
    "mocha": "2.4.5",
    "moment": "^2.11.2",
    "node-sass": "3.4.2",
    "react": "0.14.7",
    "react-addons-pure-render-mixin": "0.14.7",
    "react-dom": "0.14.7",
    "sass-loader": "3.1.2",
    "socket.io-client": "1.4.5",
    "style-loader": "0.13.0",
    "webpack": "1.12.13"
  }
}
