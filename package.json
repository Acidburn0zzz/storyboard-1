{
  "name": "storyboard",
  "version": "0.1.0",
  "description": "End-to-end, hierarchical, real-time, colorful logs and stories",
  "main": "lib/storyboard.js",
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "keywords": [
    "log",
    "logging",
    "websockets",
    "console",
    "isomorphic"
  ],
  "homepage": "https://github.com/guigrpa/storyboard#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/storyboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/storyboard.git"
  },
  "scripts": {
    "compile": "rm -rf lib && coffee --no-header -o lib -c src/storyboard.coffee && coffee --no-header -o lib/gral -c src/gral && coffee --no-header -o lib/listeners -c src/listeners && coffee --no-header -o lib/vendor -c src/vendor",
    "testLibDev": "cross-env NODE_ENV=development istanbul cover node_modules/mocha/bin/_mocha --report json -- --opts test/lib/mocha.opts && mv coverage/coverage-final.json coverage/coverage-DEVELOPMENT.json && rm coverage/coverage.json",
    "testLibProd": "cross-env NODE_ENV=production istanbul cover node_modules/mocha/bin/_mocha --report json -- --opts test/lib/mocha.opts && mv coverage/coverage-final.json coverage/coverage-PRODUCTION.json && rm coverage/coverage.json",
    "buildServerLogsApp": "cross-env NODE_ENV=production webpack --config src/serverLogsApp/webpackConfig.coffee --colors --progress --display-modules --display-chunks -p",
    "buildServerLogsAppWatch": "webpack --config src/serverLogsApp/webpackConfig.coffee --colors --progress --display-modules --display-chunks --watch",
    "buildExtension": "cross-env NODE_ENV=production webpack --config src/chromeExtension/webpackConfig.coffee --colors --progress --display-modules --display-chunks -p",
    "buildExtensionWatch": "webpack --config src/chromeExtension/webpackConfig.coffee --colors --progress --display-modules --display-chunks --watch",
    "zipExtension": "rm -f chromeExtension/chromeExtension_*.zip && bestzip chromeExtension_v0.1.0.zip chromeExtension/* && mv chromeExtension_v0.1.0.zip chromeExtension/",
    "buildExample": "webpack --config src/example/webpackConfig.coffee --colors --progress --display-modules --display-chunks",
    "buildExampleWatch": "webpack --config src/example/webpackConfig.coffee --colors --progress --display-modules --display-chunks --watch",
    "example": "coffee src/example/server.coffee",
    "build": "coffee package.coffee && npm run compile && npm run buildServerLogsApp && npm run buildExtension && npm run test && npm run zipExtension",
    "test": "npm run testCov",
    "testCov": "rm -rf coverage && npm run testLibDev && npm run testLibProd && npm run testCovMerge",
    "testCovMerge": "coffee tools/mergeCoverage.coffee"
  },
  "dependencies": {
    "bluebird": "3.3.1",
    "chalk": "1.1.1",
    "express": "4.13.4",
    "lodash": "4.5.0",
    "node-uuid": "1.4.7",
    "socket.io": "1.4.5",
    "timm": "0.4.3"
  },
  "devDependencies": {
    "babel-core": "6.6.5",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "bestzip": "1.1.3",
    "body-parser": "1.15.0",
    "chai": "3.5.0",
    "cjsx-loader": "2.1.0",
    "coffee-coverage": "1.0.1",
    "coffee-loader": "0.7.2",
    "coffee-script": "1.10.0",
    "coveralls": "2.11.6",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "diveSync": "0.3.0",
    "envify": "3.4.0",
    "file-loader": "0.8.5",
    "font-awesome": "4.5.0",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "moment": "^2.11.2",
    "node-sass": "3.4.2",
    "react": "15.0.0-rc.1",
    "react-addons-perf": "15.0.0-rc.1",
    "react-addons-pure-render-mixin": "15.0.0-rc.1",
    "react-dom": "15.0.0-rc.1",
    "react-redux": "4.4.1",
    "redux": "3.3.1",
    "redux-devtools": "3.1.1",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-inspector": "0.3.1",
    "redux-devtools-log-monitor": "1.0.5",
    "redux-logger": "2.6.1",
    "redux-saga": "0.9.4",
    "redux-thunk": "1.0.3",
    "sass-loader": "3.1.2",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0",
    "socket.io-client": "1.4.5",
    "style-loader": "0.13.0",
    "tinycolor2": "1.3.0",
    "uglifyjs": "2.4.10",
    "webpack": "1.12.13"
  }
}
